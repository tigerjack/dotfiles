<?xml version="1.0" encoding="ISO-8859-1"?>
<!--The Xournal++ settings file. Do not edit this file! The most settings are available in the Settings dialog, the others are commented in this file, but handle with care!-->
<settings>
  <property name="useXinput" value="true"/>
  <property name="presureSensitivity" value="true"/>
  <property name="ignoreCoreEvents" value="false"/>
  <property name="selectedToolbar" value="Xournal++"/>
  <property name="lastSavePath" value="file:///mnt/internal/Data"/>
  <property name="lastImagePath" value=""/>
  <property name="displayDpi" value="72"/>
  <property name="mainWndWidth" value="1916"/>
  <property name="mainWndHeight" value="923"/>
  <property name="maximized" value="false"/>
  <property name="showSidebar" value="false"/>
  <property name="sidebarWidth" value="1"/>
  <property name="sidebarOnRight" value="false"/>
  <property name="scrollbarOnLeft" value="false"/>
  <property name="showTwoPages" value="false"/>
  <property name="presentationMode" value="false"/>
  <!--Which gui elements are hidden if you are in Fullscreen mode, separated by a colon (,)-->
  <property name="fullscreenHideElements" value="mainMenubar"/>
  <!--Which gui elements are hidden if you are in Presentation mode, separated by a colon (,)-->
  <property name="presentationHideElements" value="mainMenubar,sidebarContents"/>
  <property name="showBigCursor" value="false"/>
  <property name="scrollbarHideType" value="none"/>
  <!--Hides scroolbars in the main window, allowed values: "none", "horizontal", "vertical", "both"-->
  <property name="autoloadPdfXoj" value="true"/>
  <property name="defaultSaveName" value="%F-Note-%H-%M.xoj"/>
  <!--This paper format is visible in the paper format dialog, separated by a colon-->
  <property name="visiblePageFormats" value="iso_a4,iso_a5,na_letter,na_legal"/>
  <property name="autosaveEnabled" value="true"/>
  <property name="autosaveTimeout" value="3"/>
  <property name="addHorizontalSpace" value="false"/>
  <property name="addVerticalSpace" value="false"/>
  <property name="fixXinput" value="false"/>
  <!--If Xournal crashes if you e.g. unplug your mouse set this to true. If you have input problems, you can turn it of with false.-->
  <property name="enableLeafEnterWorkaround" value="true"/>
  <property name="pageInsertType" value="copyPage"/>
  <property name="pageBackgroundColor" value="3421236"/>
  <property name="selectionColor" value="16711680"/>
  <!--The count of rendered PDF pages which will be cached.-->
  <property name="pdfPageCacheSize" value="10"/>
  <!--The multiplier for the pressure sensitivity of the pen-->
  <property name="widthMinimumMultiplier" value="0.000"/>
  <!--The multiplier for the pressure sensitivity of the pen-->
  <property name="widthMaximumMultiplier" value="1.250"/>
  <property name="font" font="Sans" size="12.0"/>
  <data name="buttonConfig">
    <data name="default">
      <attribute name="arrow" type="boolean" value="false"/>
      <attribute name="changeRulerShapeRecognizer" type="boolean" value="true"/>
      <attribute name="circle" type="boolean" value="false"/>
      <attribute name="color" type="hex" value="000000"/>
      <attribute name="rectangle" type="boolean" value="false"/>
      <attribute name="ruler" type="boolean" value="false"/>
      <attribute name="shapeRecognizer" type="boolean" value="false"/>
      <attribute name="size" type="string" value="thin"/>
      <attribute name="tool" type="string" value="pen"/>
    </data>
    <data name="eraser">
      <attribute name="tool" type="string" value="none"/>
    </data>
    <data name="middle">
      <attribute name="eraserMode" type="string" value="none"/>
      <attribute name="tool" type="string" value="eraser"/>
    </data>
    <data name="right">
      <attribute name="tool" type="string" value="none"/>
    </data>
    <data name="touch">
      <attribute name="device" type="string" value=""/>
      <attribute name="disableDrawing" type="boolean" value="false"/>
      <attribute name="tool" type="string" value="none"/>
    </data>
  </data>
  <!--Available values are: system, A4, Letter, Custom: For custom you have to create the tags width and height.-->
  <data name="format">
    <attribute name="paperformat" type="string" value="system"/>
    <attribute name="unit" type="string" value="cm"/>
  </data>
  <data name="lastUsedPageBgColor">
    <attribute name="color00" type="hex" value="343434"/>
    <attribute name="count" type="hex" value="000001"/>
  </data>
  <data name="tools">
    <attribute name="current" type="string" value="pen"/>
    <data name="eraser">
      <attribute name="size" type="string" value="MEDIUM"/>
      <attribute name="type" type="string" value="default"/>
    </data>
    <data name="hand"/>
    <data name="hilighter">
      <attribute name="arrow" type="boolean" value="false"/>
      <attribute name="circle" type="boolean" value="false"/>
      <attribute name="color" type="hex" value="ffff00"/>
      <attribute name="rectangle" type="boolean" value="false"/>
      <attribute name="ruler" type="boolean" value="false"/>
      <attribute name="shapeRecognizer" type="boolean" value="false"/>
      <attribute name="size" type="string" value="MEDIUM"/>
    </data>
    <data name="image"/>
    <data name="pen">
      <attribute name="arrow" type="boolean" value="false"/>
      <attribute name="circle" type="boolean" value="false"/>
      <attribute name="color" type="hex" value="ffffff"/>
      <attribute name="rectangle" type="boolean" value="false"/>
      <attribute name="ruler" type="boolean" value="false"/>
      <attribute name="shapeRecognizer" type="boolean" value="false"/>
      <attribute name="size" type="string" value="MEDIUM"/>
    </data>
    <data name="selectObject"/>
    <data name="selectRect"/>
    <data name="selectRegion"/>
    <data name="text">
      <attribute name="color" type="hex" value="000000"/>
    </data>
    <data name="verticalSpace"/>
  </data>
</settings>
